    "CustomPolicy": "{\"Version\":\"2012-10-17\",\"Statement\":[{\"Effect\":\"Deny\",\"Action\":[\"sso-directory:DeleteUser\",\"sso:Put*\",\"sso-directory:DeleteProvisioningTenant\",\"sso-directory:DeleteBearerToken\"],\"Resource\":\"*\"}]}"
}