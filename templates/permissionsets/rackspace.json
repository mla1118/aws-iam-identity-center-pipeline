{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "support:*",
                "account:*",
                "aws-portal:*Billing",
                "aws-portal:*PaymentMethods",
                "aws-portal:ViewAccount",
                "aws-portal:ViewUsage",
                "billing:*",
                "budgets:*",
                "ce:*",
                "consolidatedbilling:*",
                "cur:*",
                "freetier:*",
                "invoicing:*",
                "payments:*",
                "purchase-orders:*",
                "tax:*",
                "savingsplans:DescribeSavingsPlans",
                "organizations:List*",
                "organizations:Describe*",
                "organizations:AcceptHandshake",
                "organizations:DeclineHandshake",
                "organizations:CreateOrganization"
            ],
            "Resource": "*",
            "Effect": "Allow",
            "Sid": "LeastPrivilege"
        },
        {
            "Action": [
                "s3:CreateBucket",
                "s3:PutBucketPolicy",
                "s3:PutBucketPublicAccessBlock",
                "s3:PutEncryptionConfiguration",
                "s3:List*",
                "s3:Get*"
            ],
            "Resource": [
                "arn:aws:s3:::cbd9a3e73f224d25b5bef157b5c5c9a4-aws-cur",
                "arn:aws:s3:::cbd9a3e73f224d25b5bef157b5c5c9a4-aws-cur/*",
                "arn:aws:s3:::cdt-calsaws4-aws-payer-jhc-billing-reports",
                "arn:aws:s3:::cdt-calsaws4-aws-payer-jhc-billing-reports/*"
            ],
            "Effect": "Allow",
            "Sid": "AllowAccessToCURBucketForBilling"
        },
        {
            "Action": [
                "s3:ListAllMyBuckets"
            ],
            "Resource": "*",
            "Effect": "Allow"
        },
        {
            "Action": [
                "s3:GetBucketLocation",
                "s3:GetObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::cbd9a3e73f224d25b5bef157b5c5c9a4-logs",
                "arn:aws:s3:::cbd9a3e73f224d25b5bef157b5c5c9a4-logs/*",
                "arn:aws:s3:::/",
                "arn:aws:s3:::/*"
            ],
            "Effect": "Allow",
            "Sid": "AllowAccessToCloudTrailBucket"
        },
        {
            "Action": "sts:AssumeRole",
            "Resource": "arn:aws:iam::*:role/*",
            "Effect": "Allow",
            "Sid": "AssumeRoleOnMemberAccounts"
        },
        {
            "Action": [
                "wellarchitected:*",
                "organizations:*",
                "aws-portal:ModifyAccount",
                "account:*"
            ],
            "Resource": "*",
            "Effect": "Allow",
            "Sid": "OtherPermissions"
        }
    ]
}